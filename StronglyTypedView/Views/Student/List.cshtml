@using StronglyTypedView.Models;
@model IEnumerable<Student>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<hr />
<table border="1">
    <tr>
        <th>RollNo</th> 
        <th>Name</th>
        <th>Address</th>
        <th>Course</th>
    </tr>
    @foreach(Student s in Model)
        {
            <tr>
                <td>@s.Rollno</td>
                <td>@s.Name</td>
                <td>@s.Address</td>
                <td>@s.Course</td>
            </tr>
        }
</table>